{
  "name": "fast-food-fast-frontend",
  "version": "1.0.0",
  "description": "Fast food fast React frontend",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest --u --coverage --colors --verbose",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "test:watch": "jest --watch",
    "start:dev": "webpack-dev-server --mode development ./src/index.js --config ./webpack.config.js --output ./dist/bundle.js --open",
    "build": "webpack --mode production ./src/index.js --output ./dist/bundle.js",
    "heroku-prebuild": "npm run-script install",
    "heroku-postbuild": "npm run-script build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Proception/fast-food-fast-frontend.git"
  },
  "author": "proception",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Proception/fast-food-fast-frontend/issues"
  },
  "homepage": "https://github.com/Proception/fast-food-fast-frontend#readme",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "express": "^4.16.4",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1",
    "sinon": "^7.2.5",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "connected-react-router": "^6.3.1",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "jwt-decode": "^2.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "moxios": "^0.4.0",
    "react-redux": "^6.0.0",
    "react-test-renderer": "15.4.0",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "style-loader": "^0.23.1",
    "sweetalert": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.1",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>__tests__/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/mocks/fileMock.js"
    }
  }
}
